{
  "name": "tiny-bilibili-ws",
  "type": "module",
  "version": "0.1.10",
  "packageManager": "pnpm@8.9.2",
  "description": "Bilibili Live WebSocket/TCP API",
  "author": "starknt",
  "license": "MIT",
  "homepage": "https://github.com/starknt/tiny-bilibili-ws#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starknt/tiny-bilibili-ws.git"
  },
  "bugs": "https://github.com/starknt/tiny-bilibili-ws/issues",
  "keywords": [
    "ws",
    "websocket",
    "bilibili",
    "live"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "require": "./dist/browser.cjs",
      "import": "./dist/browser.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "browser.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "dev": "unbuild --stub",
    "docs:dev": "pnpm -C docs run docs:dev",
    "docs:build": "pnpm -C docs run docs:build",
    "docs:preview": "pnpm -C docs run docs:preview",
    "test": "vitest",
    "bench": "vitest bench",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "size": "pnpm run build && tsx --tsconfig=./tsconfig.tsx.json ./scripts/size.ts",
    "g": "tsx --tsconfig=./tsconfig.tsx.json ./scripts/g-cmd.ts",
    "test:memory:leak": "node --inspect ./scripts/m-test.mjs"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@types/node": "^20.8.7",
    "@types/pako": "^2.0.2",
    "@types/ws": "^8.5.8",
    "brotli-size": "^4.0.0",
    "bumpp": "^9.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eventemitter3": "5.0.1",
    "pnpm": "^8.9.2",
    "terser": "^5.22.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  }
}
